{
  "name": "deepwater-boilerplate",
  "version": "4.0.0",
  "description": "Api boilerplate",
  "main": "server/main.js",
  "readme": "README.md",
  "author": {
    "name": "Jean Peixoto",
    "email": "jeanpeixotov@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "yarn compile &&  node bin/server/main.js",
    "dev": "yarn compile && concurrently -r \"yarn watch:server\" \"yarn nodemon\"",
    "compile": "yarn tsc",
    "watch:server": "yarn tsc -w --preserveWatchOutput",
    "pretest": "yarn compile",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "cross-env ENV=test jest --detectOpenHandles --colors --testPathIgnorePatterns bin",
    "test:watch": "cross-env ENV=test jest --watch --detectOpenHandles --colors --testPathIgnorePatterns bin",
    "test:pipeline": "yarn compile && cross-env ENV=test jest --detectOpenHandles --colors bin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn test\" \"yarn tslint -p tsconfig.json\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.639.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.6.0",
    "express": "^4.17.1",
    "http-aws-es": "^6.0.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/elasticsearch": "^5.0.36",
    "@types/express": "^4.16.0",
    "@types/http-aws-es": "^6.0.0",
    "@types/jest": "^25.1.4",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.9.0",
    "@types/supertest": "^2.0.8",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^1.0.0-rc.8",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "nock": "^12.0.2",
    "nodemon": "^1.18.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3",
    "watch": "^1.0.2"
  }
}